{
  "name": "@platforma-open/milaboratories.gene-annotations-assets",
  "version": "1.1.0",
  "description": "Gene annotations for various species",
  "scripts": {
    "cleanup": "rm -rf ./pkg-*.tgz && rm -rf ./dist/",
    "build": "pl-pkg build"
  },
  "block-software": {
    "entrypoints": {
      "homo-sapiens": {
        "reference": "@platforma-open/milaboratories.gene-annotations.homo-sapiens/dist/tengo/asset/main.as.json"
      },
      "mus-musculus": {
        "reference": "@platforma-open/milaboratories.gene-annotations.mus-musculus/dist/tengo/asset/main.as.json"
      },
      "rattus-norvegicus": {
        "reference": "@platforma-open/milaboratories.gene-annotations.rattus-norvegicus/dist/tengo/asset/main.as.json"
      },
      "danio-rerio": {
        "reference": "@platforma-open/milaboratories.gene-annotations.danio-rerio/dist/tengo/asset/main.as.json"
      },
      "drosophila-melanogaster": {
        "reference": "@platforma-open/milaboratories.gene-annotations.drosophila-melanogaster/dist/tengo/asset/main.as.json"
      },
      "arabidopsis-thaliana": {
        "reference": "@platforma-open/milaboratories.gene-annotations.arabidopsis-thaliana/dist/tengo/asset/main.as.json"
      },
      "saccharomyces-cerevisiae": {
        "reference": "@platforma-open/milaboratories.gene-annotations.saccharomyces-cerevisiae/dist/tengo/asset/main.as.json"
      },
      "caenorhabditis-elegans": {
        "reference": "@platforma-open/milaboratories.gene-annotations.caenorhabditis-elegans/dist/tengo/asset/main.as.json"
      },
      "gallus-gallus": {
        "reference": "@platforma-open/milaboratories.gene-annotations.gallus-gallus/dist/tengo/asset/main.as.json"
      },
      "bos-taurus": {
        "reference": "@platforma-open/milaboratories.gene-annotations.bos-taurus/dist/tengo/asset/main.as.json"
      },
      "sus-scrofa": {
        "reference": "@platforma-open/milaboratories.gene-annotations.sus-scrofa/dist/tengo/asset/main.as.json"
      }
    }
  },
  "files": [
    "dist/"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@platforma-open/milaboratories.gene-annotations.homo-sapiens": "workspace:*",
    "@platforma-open/milaboratories.gene-annotations.mus-musculus": "workspace:*",
    "@platforma-open/milaboratories.gene-annotations.rattus-norvegicus": "workspace:*",
    "@platforma-open/milaboratories.gene-annotations.danio-rerio": "workspace:*",
    "@platforma-open/milaboratories.gene-annotations.drosophila-melanogaster": "workspace:*",
    "@platforma-open/milaboratories.gene-annotations.arabidopsis-thaliana": "workspace:*",
    "@platforma-open/milaboratories.gene-annotations.saccharomyces-cerevisiae": "workspace:*",
    "@platforma-open/milaboratories.gene-annotations.caenorhabditis-elegans": "workspace:*",
    "@platforma-open/milaboratories.gene-annotations.gallus-gallus": "workspace:*",
    "@platforma-open/milaboratories.gene-annotations.bos-taurus": "workspace:*",
    "@platforma-open/milaboratories.gene-annotations.sus-scrofa": "workspace:*"
  },
  "devDependencies": {
    "@platforma-sdk/package-builder": "catalog:"
  }
}